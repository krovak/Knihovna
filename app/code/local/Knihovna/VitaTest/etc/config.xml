<?xml version="1.0"?>
<config>
    <modules>
        <Knihovna_VitaTest>
            <version>0.1.0</version>
        </Knihovna_VitaTest>
    </modules>
    <frontend>
        <layout>
            <updates>
                <knihovna_vitatest>
                    <file>ahojVita.xml</file>
                </knihovna_vitatest>
            </updates>
        </layout>
    </frontend>
    <global>
        <models>
            <knihovna_vitatest>
                <class>Knihovna_VitaTest_Model</class>
                <resourceModel>knihovna_vitatest_resource</resourceModel>
            </knihovna_vitatest>
            <knihovna_vitatest_resource>
                <entities>
                    <knihovna_vitatest>
                        <table>tabulka_vita</table>
                    </knihovna_vitatest>
                </entities>
            </knihovna_vitatest_resource>

        </models>
        <blocks>
            <knihovna_vitatest>
                <class>Knihovna_VitaTest_Block</class>
            </knihovna_vitatest>
        </blocks>
        <helpers>
            <knihovna_vitatest>
                <class>Knihovna_VitaTest_Helper</class>
            </knihovna_vitatest>
        </helpers>
        <resources>
            <knihovna_vitatest_setup>
                <setup>
                    <module>Knihovna_VitaTest</module>
                </setup>
            </knihovna_vitatest_setup>
        </resources>
    </global>
</config>