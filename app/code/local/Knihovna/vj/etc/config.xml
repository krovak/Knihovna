<?xml version="1.0"?>
<config>
    <modules>
        <Knihovna_VJ>
            <version>0.1.0</version>
        </Knihovna_VJ>
    </modules>
    <admin>
        <routers>
            <adminhtml>
                <args>
                    <modules>
                        <Knihovna_vj_Adminhtml before='Mage_Adminhtml'>Knihovna_vj_Adminhtml</Knihovna_vj_Adminhtml>
                    </modules>
                </args>
            </adminhtml>
        </routers>
    </admin>
    <frontend>
        <routers>
            <vj>
                <use>standard</use>
                <args>
                    <module>Knihovna_vj</module>
                    <frontName>vj</frontName>
                </args>
            </vj>
        </routers>
    </frontend>
    <global>
        <models>
            <vj>
                <class>Knihovna_vj_Model</class>
                <resourceModel>vj_resource</resourceModel>
            </vj>
            <vj_resource>
                <class>Knihovna_vj_Model_Resource</class>
                <entities>
                    <knihovna_vj>
                        <table>vj_test</table>
                    </knihovna_vj>
                </entities>
            </vj_resource>
        </models>
        <blocks>
            <vj>
                <class>Knihovna_vj_Block</class>
            </vj>
        </blocks>
        <helpers>
            <vj>
                <class>Knihovna_vj_Helper</class>
            </vj>
        </helpers>
        <resources>
            <vj_setup>
                <setup>
                    <module>Knihovna_vj</module>
                </setup>
            </vj_setup>
        </resources>
    </global>
</config>